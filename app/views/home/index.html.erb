<span class="success"><%= flash[:notice] %></span>
<span class="fail"><%= flash[:alert] %></span>

<h1>Welcome to my Website</h1>

<% if @users.any? %>
  <% @users.each do |user| %>
    <ul>
      <li><%= user.email %></li>
    </ul>
  <% end %>
<% end %>

<%= link_to "Add New User", new_user_path, :class => "btn btn-success" %>

<%= link_to "Log In", log_in_path, :class => "btn btn-primary" %>

<%= link_to "Log Out", log_out_path, :class => "btn btn-warning" %>
